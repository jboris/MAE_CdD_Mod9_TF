        "df_aux['review_rate'] = pd.to_numeric(df_aux['review_rate'], errors='coerce')\n",
        "\n",
        "# 2. Definir la función para categorizar el sentimiento\n",
        "def get_sentiment(rate):\n",
        "    if rate >= 6:\n",
        "        return 'positivo'\n",
        "    elif rate <= 4:\n",
        "        return 'negativo'\n",
        "    else:\n",
        "        return None # Descartar 5s (Neutral)\n",
        "\n",
        "# 3. Aplicar la función y crear la columna 'sentimiento'\n",
        "df_aux['sentiment'] = df_aux['review_rate'].apply(get_sentiment)\n",
        "\n",
        "# 4. Filtrar el DataFrame para descartar los valores nulos (las reseñas con nota 5)\n",
        "df_filtrado = df_aux.dropna(subset=['sentiment'])\n",
        "\n",
        "# 5. Seleccionar solo las columnas necesarias para el DataFrame final\n",
        "df_desbalanceado = df_filtrado[['sentiment', 'review_text']].copy()\n",
        "\n",
        "# 6. Renombrar las columnas para que coincidan con lo solicitado\n",
        "df_desbalanceado.columns = ['sentiment', 'review']\n",
        "\n",
        "print(\"--- DataFrame Inicial (Filtrado por Sentimiento) ---\")\n",
        "print(df_desbalanceado['sentiment'].value_counts())\n",
        "print(\"-\" * 40)"
